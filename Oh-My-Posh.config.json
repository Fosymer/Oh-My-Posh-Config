{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "style": "folder"
          },
          "foreground": "#ffffff",
          ;"powerline_symbol": "\ue0b0",
          "background": "accent",
          "type": "path",
          "style": "powerline"
        }
      ]
    }
  ],
  "version": 3,
  "shell_integration": false,
  "final_space": true
}
